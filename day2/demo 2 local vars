
public class Demo1 {
	
	//local
	//instance
	//class variable
	
	int instVar; //instance
	
	void disp() {
		
		int test; //local var
		test=9;
		System.out.println(test);
		System.out.println(instVar);
		
	}
	
	public static void main(String[] args) {	
		
		Demo1 demo1=new Demo1();
			demo1.disp();
	}
	
	
}
