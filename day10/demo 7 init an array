	//int marks5;
	
	int[] marks= {3,6,8,7,9};
	
	
	
	void disp()
	{
		
		System.out.println("marks [3] "+marks[3]);
		
		for(int a:marks)
		{
			System.out.println(a);
		}
	
	}
	
	public static void main(String[] args) {	

		new Demo1().disp();
		
