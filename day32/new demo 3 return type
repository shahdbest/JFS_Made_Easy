
@FunctionalInterface
interface TestInterface
{

	void abc();
}



public class Java8Demos{

	public static int myMethod() {
		
		System.out.println("this is my method demo");
		return 80;
	}
	
	
	public static void main(String[] args) {
		
		TestInterface t1=Java8Demos::myMethod;
		t1.abc();
		
		//int x=t1.abc(); error abc is void
		
	}
	
	
}
