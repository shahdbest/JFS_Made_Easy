
public class Demo1 {
	public static void main(String[] args) {	
		Demo1 demo1=new Demo1();
		myFunction();
		//disp(); error
		
		demo1.disp();
	
	}
	
	static void myFunction() {
				System.out.println("Hello world!! this my function");
	
	}
	
	void disp()
	{
		System.out.println("disp method");
	}
	
}
