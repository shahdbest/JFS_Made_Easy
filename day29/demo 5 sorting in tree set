
// key and a value pair
//keys are unique
// 586101 -> bijapur 
//captain -> players  Map<String,List<Player>>
//team-players

import java.util.Collections;
import java.util.HashMap;
import java.util.Hashtable;
import java.util.Iterator;
import java.util.LinkedHashMap;
import java.util.Map;
import java.util.Map.Entry;
import java.util.Set;
import java.util.TreeMap;

public class MapsDemo {

	public static void main(String[] args) {

		TreeMap<Integer, String> players=new TreeMap<Integer, String>(Collections.reverseOrder());
		
		
		players.put(18, "virat");
		players.put(45, "rohit");
		players.put(17, "abd");
		players.put(10, "sachin");
		players.put(1, "kl rahul");
		
		
		 	for(Map.Entry<Integer, String> entry :players.entrySet()) {
		 		
		 		System.out.println(entry.getKey() +" "+ entry.getValue());
		 	
		 	}
		 	
	}
	
	

}
