
@FunctionalInterface
interface TestInterface{
	float converter(float dollar);
	
}


public class Java8Demos{

	public static void main(String[] args) {
	
	
		TestInterface t=(dollar)->{
			float res=dollar*87;
			return res;
		};
		
		float val= t.converter(10);
		System.out.println(val +"rupees");
	}
}
