package packone;

//public : visible to everyone
//private visible within the class where its declared 

public class C1 {

	
	private int x=10;
	
	public void disp()
	{
		System.out.println("c1 class disp method");
		System.out.println(x);
	}
	
}

-----------------------------------------


  package packone;

public class C2 {

	
	public static void main(String[] args) {
		
		C1 c1=new C1();
		c1.disp();
	//	System.out.println(c1.x);error x is private cannot access it here
	}
}

