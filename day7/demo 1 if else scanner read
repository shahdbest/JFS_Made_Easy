
// if-else -- switch case

import java.util.Scanner;

public class Demo1 {
	
	public static void main(String[] args) {	

		
		Scanner sc=new Scanner(System.in);
	
		System.out.println("enter age");
		int age=sc.nextInt();
	
		if(age>18)
			System.out.println("employee eligible");
		else
			System.out.println("employee not eligible");
	}
	
	
}
