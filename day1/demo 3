
public class Demo1 {

	// int main=10;
	//primary thread (main) -> execution path of a program
	public static void main(String[] args) {	
		Demo1 demo1=new Demo1();
		System.out.println("Hello world");
		
		String name=Thread.currentThread().getName();
	System.out.println("name of the primary thread is -> "+name);
		
	}
	// child thread- > Thread class - Runnable interface
	public static void myMain(String[] args) {
				System.out.println("Hello world!! this my main");
	
	}
	
}
