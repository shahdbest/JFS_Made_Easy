
public class Calculator {

	
	void add(int a,int b)
	{
		
		System.out.println(a + b);
	}
	
}


-----------------------------


  

public class Demo1 {

	
	
	public static void main(String[] args) {	


		Calculator calculator=new Calculator();
		calculator.add(12,20);
		calculator.add(15,20);
	}
}
