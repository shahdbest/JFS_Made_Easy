
//if-else -- switch 

import java.util.Scanner;

public class Demo1 {
	
	public static void main(String[] args) {	

		
		int n1=100;
		int n2=11;
		int n3=777;
		
		if(n1> n2)
		{
			if(n1>n3)
				System.out.println(n1 +"is the largest");
			else
				System.out.println(n3 +"is the largest");
				
		}
		else
		{
			if(n2>n3)
				System.out.println(n2 +"is the largest");
			
			else
				System.out.println(n3 +"is the largest");
			
		}
		

		
	}
	
	
}
