
@FunctionalInterface
interface TestInterface
{

	void sum(int x,int y);
}



public class Java8Demos{

	public Java8Demos(int x,int y) {
		
		System.out.println("this is constructor demo "+ (x+y));
	}
	
	
	public static void main(String[] args) {
		
		
		
		TestInterface t2=Java8Demos::new; 
		t2.sum(40,70);
		
		
	}
	
	
}
