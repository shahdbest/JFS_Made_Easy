
//collection of elements of same type 

public class Demo1 {

//	int marks1;
	//int marks2;
	//int marks3;
//	int marks4;
	//int marks5;
	
	int[] marks=new int[5];
	
	
	
	void disp()
	{
		
		marks[0]=5;
		marks[1]=55;
		marks[2]=15;
		
		
		for(int i=0;i<marks.length;i++)
		{
			System.out.println("i" + i);
			System.out.println(marks[i]);
		}
	
	}
	
	public static void main(String[] args) {	

		new Demo1().disp();
		
		//Demo1 d1=new Demo1();
		//d1.disp();
	}
}
