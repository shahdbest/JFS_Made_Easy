
@FunctionalInterface
interface TestInterface{
	void disp(String msg);
	
}


public class Java8Demos{

	public static void main(String[] args) {
	
		TestInterface t= (String msg)-> {
					
				System.out.println("disp "+msg);
			};
			
		t.disp("hello world");
	
		TestInterface t1= (msg)-> {
			
			System.out.println("disp "+msg);
		};
		
		t1.disp("hello world");

		
		
		
	}
}
