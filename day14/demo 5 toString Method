// pojo -- model -- bean- entity

class Employee
{

	private String emplayeeName;
	private int age;
	
	public String getEmplayeeName() {
		return emplayeeName;
	}
	public void setEmplayeeName(String emplayeeName) {
		this.emplayeeName = emplayeeName;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public Employee(String emplayeeName, int age) {
		super();
		this.emplayeeName = emplayeeName;
		this.age = age;
	}
	
	public Employee() {
		// TODO Auto-generated constructor stub
	}
	
	@Override
	public String toString() {
		return "Employee [emplayeeName=" + emplayeeName + ", age=" + age + "]";
	}
	
}


public class DemoJava {

	public static void main(String[] args) {
	
		Employee employee=new Employee("Rohit", 40);
		
		System.out.println(employee);
		
	}	

	
}
