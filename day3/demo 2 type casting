
//Datatype 

//primitive type - simple values 
/* 8bits , 16bits , 32bits , 64bits
 * byte , short , int ,long
 * float , double
 * char
 * boolean 
 * */


//class type : objects 

public class Demo1 {
	
	public static void main(String[] args) {	
		
		byte a=90; //8 bits
		int b=a; //implicit //32 bits
		
		System.out.println(b);
	
		int c=80;
		byte d=(byte)c; //explicit 
		System.out.println(d);
			
	
		int e=256;
		byte f=(byte)e; //chance of loss of data
		
		System.out.println(f);
		
	}
	
	
}
