
//if-else -- switch 

import java.util.Scanner;

public class Demo1 {
	
	public static void main(String[] args) {	

		Scanner sc=new Scanner(System.in);
		
		System.out.println("eneter age");
		int age= sc.nextInt();
		
		if(age > 18)
		System.out.println("selected");
		else
			System.out.println("rejected");
	}
	
	
}
