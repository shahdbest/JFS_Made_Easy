
//Datatype 

//primitive type - simple values 
/* 8bits , 16bits , 32bits , 64bits
 * byte , short , int ,long  -> 0
 * float , double -> 0.0
 * char ->  /u0000
 * boolean -> false
 * */


//class type : objects  -> null by def
// Boxing -> primitive to class
// wrapper classes

/*
 * byte -Byte
 * short -Short
 * int - Integer
 * long ->Long
 * char - Character
 * boolean -Boolean
 * float - Float
 * double -Double
 * 
 * */

public class Demo1 {
	
	static boolean z;
	static Boolean y;

	public static void main(String[] args) {	

		System.out.println(z);		
		System.out.println(y);		
		
		String t="test";
		System.out.println(t);
		
		String strNum="10";
			
		System.out.println(strNum + 55);
	//	int num=(int)strNum; cannot be converted without a wrapper class
	//coz string is class type and int is primitive type
		
		int num=Integer.parseInt(strNum);
		
		System.out.println(num);
		System.out.println(num+55);
	}
	
}
